{
    "manifest_version": 2,
	"version": "0.0.1",
    "name": "Fixed Google Icons", 
    "description": "Changes the new Google icons back to how they should be.",
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
    "permissions": [
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
		"tabs",
        "https://*/"
    ],
    "web_accessible_resources": [
        "images/*"
    ]
}